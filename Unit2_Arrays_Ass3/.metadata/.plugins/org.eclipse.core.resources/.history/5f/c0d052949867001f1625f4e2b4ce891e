#include <stdio.h>
#include <stdlib.h>

int main(void)
{

	int r ,c,i,j ;
	int A[100][100];
	int transpose[100][100];
	printf("Enter rows and columns of the matrix \n :  ");
	scanf ("%d %d ",&r,&c);
	printf("Enter elements of matrix \n :  ");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
		  printf("a%d%d",i+1,j+1);
		  scanf("%d",&A[i][j]);
		}
	}
	for(i=0;i<r;i++)
		{
			for(j=0;j<c;j++)
			{
			  printf("%d\t",A[i][j]);
			}
			printf("\n");
		}
	return 0;
}
