#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	float num1,num2;
	printf("enter two numbers :\n");
	fflush(stdout);fflush(stdin);
	scanf("%f%f",&num1,&num2);
	fflush(stdout);fflush(stdin);
	printf("product : %.2f",num1*num2);
	return 0;

}
