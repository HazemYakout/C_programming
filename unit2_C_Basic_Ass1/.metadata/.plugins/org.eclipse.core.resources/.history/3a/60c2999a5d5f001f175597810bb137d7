#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int x=0,y=0;
	printf("Enter two integers : /n");
	fflush(stdout);fflush(stdin);
	scanf("%d /n %d",&x,&y);
	fflush(stdout);fflush(stdin);
	printf("Sum: %d",x+y);

}
