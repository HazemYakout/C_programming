#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int x,y,sum;
	printf("Enter two integers : \n");
	fflush(stdout);fflush(stdin);
	scanf("%d /n %d",&x,&y);
	fflush(stdout);fflush(stdin);
	sum =x+y;
	printf("Sum: %d",sum);

}
